{
  "manifest_version": 2,
  "name": "Shell",
  "description": "Chrome extension library to execute shell commands",
  "homepage_url": "https://github.com/alexherbo2/shell.chrome",
  "version": "1",
  "permissions": [
    "nativeMessaging"
  ],
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  }
}
